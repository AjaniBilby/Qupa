{
	"whitespace": [
		" ",
		"\n",
		"\t",
		"\r"
	],

	"comment": [
		{
			"name"  : "comment-single",
			"match" : "//*\n"
		}, {
			"name"  : "comment-multi",
			"match" : "/\\**\\*/"
		}
	],

	"global": [

		

		{
			"name"  : "import",
			"match" : "import +;"
		}, {
			"name"  : "export",
			"match" : "export +;"
		},

		{
			"name"  : "function",
			"match" : "+ +(*)*{*}"
		}
	],

	"execution": [
		{
			"name"  : "if",
			"match" : "if+(+)*{*}"
		},{
			"name"  : "elif",
			"match" : "else if+(+)*{*}"
		},{
			"name"  : "else",
			"match" : "else+{*}"
		},

		{
			"name"  : "return",
			"match" : "return*;"
		},

		{
			"name"  : "assign",
			"match" : "*=*;"
		}
	]
}