import "print" as system;

class Counter {
	int val;

	int get() {
		return this.val;
	}

	void set(int val) {
		this.val = val;
	}

	void increment() {
		this.val = this.val + 1;
	}

	void decrement() {
		this.val = this.val - 1;
	}

	void print() {
		system.print(this.val);
	}
}

void Parse(Counter a) {
	a.print(a);
	a.increment(a);
	a.print(a);
}

int main () {
	Counter a;
	a.set(3);
	a.print();
	a.decrement();
	a.print();

	Parse(a);
	a.print();
}