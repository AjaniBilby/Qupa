import "print.qp" as system;

class Counter {
	int val;

	int get() {
		return this->.val;
	}

	void set(int val) {
		this->.val = val;
		return;
	}

	void increment() {
		this->.val = this->.val + 1;
		return;
	}

	void decrement() {
		this->.val = this->.val - 1;
		return;
	}

	void print() {
		system.print(this->.val);
		return;
	}
}

