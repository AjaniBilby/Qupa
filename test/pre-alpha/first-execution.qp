/*
	Description: First executable version of Qupa
	Goals: Compiles to LLVM-IR
	  Able to do integer addition
		Print result
		Exit code 0
*/

external assume {
	type i32 size 4;
	type void size 0;

	i32 add(i32 a, i32 b);
	void print(i32 val);
}

i32 startup () {
	i32 a;
	i32 b;
	i32 c;

	a = 1;
	b = 2;
	c = add(a, b);

	return 0;
}