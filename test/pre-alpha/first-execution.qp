/*
	Description: First executable version of Qupa
	Goals: Compiles to LLVM-IR
	  Able to do integer addition
		Print result
		Exit code 0
*/


external assume {
	type i32 size 4;
	type float size 4;
	type double size 4;
	type void size 0;

	i32 iadd(i32 a, i32 b);
	float fadd(float a, float b);

	void iprint(i32 val);
	void fprint(float val);
	void iprintln(i32 val);
	void fprintln(float val);
}


i32 main () {
	i32 a;
	i32 b;
	i32 c;

	a = 1;
	b = 2;

	iprintln(a);
	iprintln(b);

	c = iadd(a, b);

	iprintln(c);

	return 1;
}
external export {
	i32 main ();
};