/*
	Description: First executable version of Qupa
	Goals: Compiles to LLVM-IR
	  Able to do integer addition
		Print result
		Exit code 0
*/


external assume {
	i32 i32_add(i32 a, i32 b);
	void i32_println(i32 val);
}


i32 main () {
	i32 a;
	i32 b;
	i32 c;

	a = 1;
	b = 2;

	i32_println(a);
	i32_println(b);

	c = i32_add(a, b);

	i32_println(c);

	return 1;
}