import "../std.qp";
/*
	Expected:
		0
		2
		3
		0
		42
		3
		0
		42
		1
*/


i32 main () {
	i32 a;
	i32 b;
	i32 c;
	bool d;

	a = 2;
	b = 3;

	c = 0;
	println(c);
	println(a);
	println(b);

	c = 2;
	d = Equal(a, c);
	if (d) {
		a = 42;
	}

	c = 0;
	println(c);
	println(a);
	println(b);

	d = Equal(a, b);
	if (d) {
		b = Add(a, b);
	} else {
		b = 1;
	}

	c = 0;
	println(c);
	println(a);
	println(b);

	return 0;
}