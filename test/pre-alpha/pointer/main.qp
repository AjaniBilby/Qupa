/*
	Expected output:
	20
	2
	20
	22
*/
import "../std.qp";

external assume {
	@i32 createInt();
};

void add(@i32 target, i32 val) {
	$target = i32_add($target, val);
	return;
}

i32 main() {
	i32 a = 20;
	@i32 b = createInt();
	$b = 2;

	i32_println(a);
	i32_println($b);

	add(b, a);

	i32_println(a);
	i32_println($b);

	return 0;
}
external export {
	i32 main();
}