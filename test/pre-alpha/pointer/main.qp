/*
	Expected output:
	20
	2
	20
	22
*/
import "alloc.qp";
import "print.qp";

void Add(@i32 target, i32 val) {
	$target = $target + val;
	return;
}

i32 main() {
	i32 a = 20;
	@char addr = malloc( 1 );
	@i32 b = static_cast#[@i32](addr);
	$b = 2;

	i32_println(a);
	i32_println($b);

	Add(b, a);

	i32_println(a);
	i32_println($b);

	return 0;
}