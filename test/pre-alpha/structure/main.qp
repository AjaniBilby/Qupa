import "../std.qp";

struct Rect {
	i32 width;
	i32 height;
}


i32 main() {
	// Define two example rectangles
	Rect a;
	Rect b;
	a.width = 4;
	a.height = 5;
	b.width = 6;
	b.height = 7;

	// Test single structure argument function
	i32 area;
	area = Area(a);
	println(area);
	area = Area(b);
	println(area);

	// Test returning struct function
	Rect c;
	c = Combine(a, b);
	println(a.width);
	println(a.height);
	println(b.width);
	println(b.height);
	println(c.width);
	println(c.height);

	return 0;
}


i32 Area(Rect rec){
	i32 area;
	area = Mul(rec.width, rec.height);
	return area;
}

Rect Combine(Rect a, Rect b) {
	a.width = Add(a.width, b.width);
	a.height = Add(a.height, b.height);
	return a;
}


external export {
	i32 main();
}